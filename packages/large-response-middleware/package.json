{
  "name": "@epilot/large-response-middleware",
  "version": "0.0.11",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epilot-dev/aws-lambda-utility-middlewares.git#main"
  },
  "keywords": [
    "aws",
    "lambda",
    "middleware",
    "large-response"
  ],
  "author": "João Pinho <jpe.pinho@gmail.com>",
  "contributors": [
    "João Pinho(https://github.com/jpinho)",
    "Diogo Marques(https://github.com/JoFont)"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "lib/",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build:declarations": "tsc -p tsconfig.json",
    "build:modules": "cross-env BABEL_ENV=bundled rollup -c rollup.config.js --bundleConfigAsCjs",
    "build": "npm-run-all -l clean -p build:modules build:declarations && npm run check-size",
    "setup-local": "./scripts/init-s3.sh",
    "pre-test": "wait-on $AWS_ENDPOINT/health && npm run setup-local",
    "test": "jest --verbose --detectOpenHandles --forceExit",
    "check-size": "size-limit",
    "lint": "npx oxlint@latest && eslint --ext .ts"
  },
  "size-limit": [
    {
      "limit": "2 kB",
      "path": "lib/index.js"
    }
  ],
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.23.3",
    "@dazn/lambda-powertools-logger": "^1.28.1",
    "@middy/core": "^2.5.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@size-limit/file": "^11.0.1",
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.816.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.9.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-visualizer": "^5.11.0",
    "size-limit": "^11.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@dazn/lambda-powertools-logger": "^1.28.1",
    "@middy/core": "^2.5.7",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.816.0"
  },
  "dependencies": {
    "core-js": "^3.34.0",
    "yn": "^5.0.0"
  }
}
